  <script>
  var Webflow = Webflow || [];
  Webflow.push(function () {
    // Check if the screen width is greater than or equal to a certain value (e.g., 768px for typical non-mobile devices)
    if (window.innerWidth >= 768) {
      ([
        {
          selector: '.tabs-menu-views', 
          delay: 7000
        }, {
          selector: '.tabs-menu',
          delay: 7000
        }, {
          selector: '.tab-menus-features',
          delay: 4000
        }
      ]).forEach(function (section) {
        // click all tab-buttons with a short interval
        const $active = $(section.selector).children('.w--current:first')
        const $siblings = $active.siblings();
        $($siblings).each(function (el, i) {
          $(this).click();
        })
        $active.click();
        let tabTimeout;
        tabLoop();
        function tabLoop() {
          // clear any existing timeout
          if (tabTimeout) clearTimeout(tabTimeout);
          tabTimeout = setTimeout(function () {
            console.log('tabLoop', section.selector);
            let $next = $(section.selector).children('a.w--current:first').next();
            if ($next.length) {
              $next.click(); // user click resets timeout
            } else {
              $next = $(section.selector).children('a').first();
              $next.click();
            }
          }, section.delay || 5000);
        }
        // click on tab-buttons, reset timeout and loop
        $(section.selector).children('a').on('click', function () {
          tabLoop();
        });
      });
    }
  });
    let circle = document.getElementById('home-hero-glow');
		let container = document.getElementById('page-wrapper'); // Use the correct ID of your parent element
		const onMouseMove = (e) => {
    		// Calculate the center coordinates of the circle
    		let centerX = e.clientX - container.getBoundingClientRect().left - (circle.offsetWidth / 2);
   		  let centerY = e.clientY - container.getBoundingClientRect().top - (circle.offsetHeight / 2);
    		// Ensure the circle stays within the container bounds
    		centerX = Math.min(Math.max(centerX, 0), container.offsetWidth - circle.offsetWidth);
    		centerY = Math.min(Math.max(centerY, 0), container.offsetHeight - circle.offsetHeight);
    		// Update the position of the circle based on the center coordinates
    		circle.style.left = centerX + 'px';
    		circle.style.top = centerY + 'px';
		}
document.addEventListener('mousemove', onMouseMove);
